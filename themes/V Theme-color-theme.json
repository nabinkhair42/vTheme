{
	"name": "V Theme",
	"type": "dark",
	"colors": {
		// Base UI colors
		"editor.background": "#2E2A4D",
		"editor.foreground": "#E5E9F0",
		"editor.selectionBackground": "#4D448A",
		"editor.selectionHighlightBackground": "#4D448A88",
		"editor.lineHighlightBackground": "#3A3461",
		"editorCursor.foreground": "#F7B32B",
		"editorWhitespace.foreground": "#3A3461",
		
		// Activity bar
		"activityBar.background": "#262341",
		"activityBar.foreground": "#E5E9F0",
		"activityBar.inactiveForeground": "#7B88A1",
		"activityBarBadge.background": "#F7B32B",
		"activityBarBadge.foreground": "#262341",
		
		// Sidebar
		"sideBar.background": "#262341",
		"sideBar.foreground": "#E5E9F0",
		"sideBarTitle.foreground": "#34B0A5",
		"sideBarSectionHeader.background": "#2E2A4D",
		"sideBarSectionHeader.foreground": "#34B0A5",
		
		// Status bar
		"statusBar.background": "#1C1A31",
		"statusBar.foreground": "#E5E9F0",
		"statusBar.noFolderBackground": "#1C1A31",
		"statusBar.debuggingBackground": "#F7B32B",
		"statusBar.debuggingForeground": "#1C1A31",
		
		// Title bar
		"titleBar.activeBackground": "#1C1A31",
		"titleBar.activeForeground": "#E5E9F0",
		"titleBar.inactiveBackground": "#1C1A31",
		"titleBar.inactiveForeground": "#7B88A1",
		
		// Tab groups
		"editorGroupHeader.tabsBackground": "#262341",
		"tab.activeBackground": "#2E2A4D",
		"tab.activeForeground": "#E5E9F0",
		"tab.activeBorder": "#F7B32B",
		"tab.inactiveBackground": "#262341",
		"tab.inactiveForeground": "#7B88A1",
		"tab.unfocusedActiveForeground": "#E5E9F0",
		"tab.unfocusedInactiveForeground": "#7B88A1",
		
		// Lists & trees
		"list.activeSelectionBackground": "#3A3461",
		"list.activeSelectionForeground": "#E5E9F0",
		"list.inactiveSelectionBackground": "#3A346180",
		"list.inactiveSelectionForeground": "#E5E9F0",
		"list.hoverBackground": "#3A346160",
		"list.highlightForeground": "#34B0A5",
		
		// Input controls
		"input.background": "#262341",
		"input.foreground": "#E5E9F0",
		"input.placeholderForeground": "#7B88A1",
		"inputOption.activeBorder": "#F7B32B",
		
		// Badge
		"badge.background": "#34B0A5",
		"badge.foreground": "#262341",
		
		// Panel
		"panel.background": "#262341",
		"panelTitle.activeBorder": "#F7B32B",
		"panelTitle.activeForeground": "#E5E9F0",
		"panelTitle.inactiveForeground": "#7B88A1",
		
		// Terminal
		"terminal.background": "#262341",
		"terminal.foreground": "#E5E9F0",
		"terminal.ansiBlack": "#1C1A31",
		"terminal.ansiBlue": "#5C78FF",
		"terminal.ansiBrightBlack": "#7B88A1",
		"terminal.ansiBrightBlue": "#7A91FF",
		"terminal.ansiBrightCyan": "#68D9CE",
		"terminal.ansiBrightGreen": "#56CF7D",
		"terminal.ansiBrightMagenta": "#DB79FF",
		"terminal.ansiBrightRed": "#FF5A5A",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightYellow": "#F7C760",
		"terminal.ansiCyan": "#34B0A5",
		"terminal.ansiGreen": "#42B462",
		"terminal.ansiMagenta": "#C64AFF",
		"terminal.ansiRed": "#E64545",
		"terminal.ansiWhite": "#E5E9F0",
		"terminal.ansiYellow": "#F7B32B",
		
		// Diff editor
		"diffEditor.insertedTextBackground": "#42B46220",
		"diffEditor.removedTextBackground": "#E6454520",
		
		// Scrollbar
		"scrollbarSlider.background": "#3A346180",
		"scrollbarSlider.hoverBackground": "#3A3461",
		"scrollbarSlider.activeBackground": "#4D448A"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7B88A1"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#E5E9F0"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#E64545"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C64AFF"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#5C78FF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#F7C760"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#34B0A5"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#F7C760"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#F7B32B"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#FF5A5A"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#56CF7D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#F7B32B"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#7A91FF"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#68D9CE"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#E64545"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E64545"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#34B0A5"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#34B0A5"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#C64AFF"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#F7B32B"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#F7B32B"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#34B0A5"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#56CF7D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#E64545"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#C64AFF"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#5C78FF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#5C78FF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#34B0A5"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E64545"
			}
		},
