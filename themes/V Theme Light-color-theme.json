{
	"name": "V Theme Light",
	"type": "light",
	"colors": {
		// Base UI colors - clean and modern light palette
		"editor.background": "#F5F8FA",
		"editor.foreground": "#2D3748",
		"editor.selectionBackground": "#C9E3F5",
		"editor.selectionHighlightBackground": "#C9E3F588",
		"editor.lineHighlightBackground": "#E9EFF5",
		"editorCursor.foreground": "#00A3A3",
		"editorWhitespace.foreground": "#E5E9F0",
		
		// Activity bar - clean and minimal
		"activityBar.background": "#FFFFFF",
		"activityBar.foreground": "#2D3748",
		"activityBar.inactiveForeground": "#718096",
		"activityBarBadge.background": "#00A3A3",
		"activityBarBadge.foreground": "#FFFFFF",
		
		// Sidebar - clean and cohesive
		"sideBar.background": "#FFFFFF",
		"sideBar.foreground": "#2D3748",
		"sideBarTitle.foreground": "#00A3A3",
		"sideBarSectionHeader.background": "#F5F8FA",
		"sideBarSectionHeader.foreground": "#00A3A3",
		
		// Status bar - refined and subtle
		"statusBar.background": "#FFFFFF",
		"statusBar.foreground": "#2D3748",
		"statusBar.noFolderBackground": "#FFFFFF",
		"statusBar.debuggingBackground": "#00A3A3",
		"statusBar.debuggingForeground": "#FFFFFF",
		
		// Title bar - clean and minimal
		"titleBar.activeBackground": "#FFFFFF",
		"titleBar.activeForeground": "#2D3748",
		"titleBar.inactiveBackground": "#FFFFFF",
		"titleBar.inactiveForeground": "#718096",
		
		// Tab groups - elegant and focused
		"editorGroupHeader.tabsBackground": "#FFFFFF",
		"tab.activeBackground": "#F5F8FA",
		"tab.activeForeground": "#2D3748",
		"tab.activeBorder": "#00A3A3",
		"tab.inactiveBackground": "#FFFFFF",
		"tab.inactiveForeground": "#718096",
		"tab.unfocusedActiveForeground": "#2D3748",
		"tab.unfocusedInactiveForeground": "#718096",
		
		// Lists & trees - refined interaction
		"list.activeSelectionBackground": "#E9EFF5",
		"list.activeSelectionForeground": "#2D3748",
		"list.inactiveSelectionBackground": "#E9EFF580",
		"list.inactiveSelectionForeground": "#2D3748",
		"list.hoverBackground": "#E9EFF560",
		"list.highlightForeground": "#00A3A3",
		
		// Input controls - clean and minimal
		"input.background": "#FFFFFF",
		"input.foreground": "#2D3748",
		"input.placeholderForeground": "#718096",
		"inputOption.activeBorder": "#00A3A3",
		
		// Badge - elegant and minimal
		"badge.background": "#00A3A3",
		"badge.foreground": "#FFFFFF",
		
		// Panel - cohesive design
		"panel.background": "#FFFFFF",
		"panelTitle.activeBorder": "#00A3A3",
		"panelTitle.activeForeground": "#2D3748",
		"panelTitle.inactiveForeground": "#718096",
		
		// Terminal - clean and readable
		"terminal.background": "#FFFFFF",
		"terminal.foreground": "#2D3748",
		"terminal.ansiBlack": "#2D3748",
		"terminal.ansiBlue": "#0072C6",
		"terminal.ansiBrightBlack": "#718096",
		"terminal.ansiBrightBlue": "#1A85FF",
		"terminal.ansiBrightCyan": "#0F9F9F",
		"terminal.ansiBrightGreen": "#26A769",
		"terminal.ansiBrightMagenta": "#C838C6",
		"terminal.ansiBrightRed": "#E03E3E",
		"terminal.ansiBrightWhite": "#F5F8FA",
		"terminal.ansiBrightYellow": "#D97706",
		"terminal.ansiCyan": "#00A3A3",
		"terminal.ansiGreen": "#16A349",
		"terminal.ansiMagenta": "#A31DB1",
		"terminal.ansiRed": "#DC2626",
		"terminal.ansiWhite": "#E5E9F0",
		"terminal.ansiYellow": "#B45309",
		
		// Diff editor - subtle but clear
		"diffEditor.insertedTextBackground": "#16A34920",
		"diffEditor.removedTextBackground": "#DC262620",
		
		// Scrollbar - refined and minimal
		"scrollbarSlider.background": "#E9EFF580",
		"scrollbarSlider.hoverBackground": "#E9EFF5",
		"scrollbarSlider.activeBackground": "#C9E3F5"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#718096"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#2D3748"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#444444"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#DC2626"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#A31DB1"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#0072C6"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#B45309"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00A3A3"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#B45309"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#B45309"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#E03E3E"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#16A349"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#B45309"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#1A85FF"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#0F9F9F"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#DC2626"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#DC2626"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00A3A3"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#00A3A3"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#A31DB1"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#B45309"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#B45309"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#00A3A3"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#16A349"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#DC2626"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#A31DB1"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#0072C6"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#0072C6"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00A3A3"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#DC2626"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#A31DB1"
			}
		},
